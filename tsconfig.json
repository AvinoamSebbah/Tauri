{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "strict": true,
      "jsx": "preserve",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "noEmit": true,
      "types": ["node"],
      "baseUrl": ".",
      "paths": {
      "@/*": ["src/*"]
    }
    },
    "include": ["src/**/*", "env.d.ts", "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue", "src/**/**/*.vue" ],
    "exclude": ["node_modules"]
  }
  